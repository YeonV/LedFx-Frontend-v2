  "build": {
    "extraResources": [
      {
        "from": "./extraResources/",
        "to": "extraResources",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "icon": "build/icon.ico",
      "artifactName": "LedFx_CC-${version}--${os}.${ext}"
    },
    "portable": {
      "artifactName": "LedFx_CC-${version}--${os}-portable.${ext}"
    },
    "nsis": {
      "artifactName": "LedFx_CC-${version}--${os}-setup.${ext}"
    },
    "linux": {
      "icon": "build/icon.png"
    },
    "mac": {
      "icon": "build/icon.png",
      "artifactName": "LedFx_CC-${version}--${os}-${arch}.${ext}"
    }
  }
